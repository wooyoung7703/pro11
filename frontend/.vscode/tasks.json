{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Frontend typecheck (vite build dry)",
			"type": "shell",
			"command": "npm run -s build",
			"isBackground": false,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		},
		{
			"label": "Build frontend (vite)",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"build"
			],
			"isBackground": false,
			"problemMatcher": [
				"$eslint - stylish"
			],
			"group": "build"
		}
	]
}